#if 1
#define PIN_XX PIN_02
//#define GPIO_PIN_02_MODE _LOW
#include "gpio/pin_02.ceu"
#elif 1
#define PIN_XX PIN_03
//#define GPIO_PIN_03_MODE _LOW
#include "gpio/pin_03.ceu"
#elif 1
#define PIN_XX PIN_10
#include "gpio/pin_10.ceu"
#endif

output bool PIN_13;

emit PIN_13(off);

var on/off v = off;
loop do
    await PIN_XX;
    v = not v;
    emit PIN_13(v);
end
